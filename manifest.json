{
    "display_information": {
        "name": "Estimatron"
    },
    "features": {
        "bot_user": {
            "display_name": "Estimatron",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/estimate",
                "url": "https://testing.com/test",
                "description": "Starts an estimation session",
                "usage_hint": "[ticket-id]",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "commands"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}